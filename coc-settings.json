{
  // proxy
  //"http.proxy": "socke5://127.0.0.1:1081",

  // coc-git
  "git.addedSign.text": "\u2588",
  //"git.addedSign.hlGroup": "DiffAdd",
  "git.changedSign.text": "\u2588",
  "git.changedSign.hlGroup": "DiffChange",
  "git.removedSign.text": "\u2588",
  "git.removedSign.hlGroup": "DiffDelete",
  "git.topRemovedSign.text": "\u2588",
  "git.topRemovedSign.hlGroup": "DiffDelete",
  "git.changeRemovedSign.text": "\u2588",
  "git.changeRemovedSign.hlGroup": "DiffChangeDelete",

  // coc-python
  "python.autoComplete.addBrackets": true,
  "python.jediEnabled": true,
  "python.linting.enabled": false,
  "python.formatting.provider": "black",
  //"python.pipenvPath": "/Users/Nordwind/Project/Python/",
  "python.venvPath": "/Users/Nordwind/.local/share/virtualenvs/Python--fREnqhY",

  // coc-tsserver
  "tsserver.enableJavascript": false,
  "tsserver.orgnizeImportOnSave": true,
  "typescript.preferences.noSemicolons": true,
  "typescript.preferences.quoteStyle": "single",
  "javascript.preferences.noSemicolons": true,
  "javascript.preferences.quoteStyle": "single",

  // coc-snippet
  "snippets.ultisnips.enable": false,

  "suggest.minTriggerInputLength": 2,
  "suggest.snippetIndicator": " â–º",
  "suggest.enablePreview": true,

  "coc.preferences.diagnostic.enable": false,
  "coc.preferences.autoTrigger": "always",
  "suggest.autoTrigger": "always",

  "languageserver":{
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    },

    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    }
  }
}
